{
    "plugins": [
      ["module:react-native-dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "blocklist": [
            "GITHUB_TOKEN"
          ],
        "allowlist": null,
        "blacklist": null, // DEPRECATED
        "whitelist": null, // DEPRECATED
        "safe": true,
        "allowUndefined": true,
        "verbose": false
      }]
    ]
  }